{% extends '::layout/layoutAdmin.html.twig' %}

{% block body %}
    <h1>Telephonique</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ telephonique.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ telephonique.titre }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ telephonique.description }}</td>
            </tr>
            <tr>
                <th>Disponible</th>
                <td>{% if telephonique.disponible %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('telephonique_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('telephonique_edit', { 'id': telephonique.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
